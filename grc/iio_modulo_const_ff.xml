<?xml version="1.0"?>
<block>
	<name>Modulo Const</name>
	<key>iio_modulo_const_ff</key>
	<category>[Industrial IO]/Math Operators</category>
	<import>import iio</import>
	<make>iio.modulo_const_ff($modulo, $vlen)</make>
	<param>
		<name>Modulo</name>
		<key>modulo</key>
		<value>1.0</value>
		<type>float</type>
	</param>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
	<check>$vlen &gt; 0</check>
	<sink>
		<name>in</name>
		<type>float</type>
		<vlen>$vlen</vlen>
	</sink>
	<source>
		<name>out</name>
		<type>float</type>
		<vlen>$vlen</vlen>
	</source>
</block>
