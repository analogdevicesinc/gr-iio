<?xml version="1.0"?>
<!--
###################################################
##Multiply Block:
##	all types, 1 output, 2 to inf inputs
###################################################
 -->
<block>
	<name>Power</name>
	<key>iio_power_ff</key>
	<category>[Industrial IO]/Math Operators</category>
	<import>from gnuradio import iio</import>
	<make>iio.power_ff($vlen)</make>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
	<check>$vlen &gt; 0</check>
	<sink>
		<name>base</name>
		<type>float</type>
		<vlen>$vlen</vlen>
	</sink>
	<sink>
		<name>exp</name>
		<type>float</type>
		<vlen>$vlen</vlen>
	</sink>
	<source>
		<name>out</name>
		<type>float</type>
		<vlen>$vlen</vlen>
	</source>
</block>
